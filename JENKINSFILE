@Library('jenkins_shared_lib') _
pipeline {
    agent any
    // Stages and other configurations...

    stages {
        stage('Git Checkout') {
            steps {
                script{
                     gitCheckout(
                     branch: "main"
                     url: "https://github.com/emmanuel5507/MavenJavaProject.git"
                     )
                }

            }
        }
   }
